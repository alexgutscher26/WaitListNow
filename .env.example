# Database URL for development
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/pingpanda?schema=public"

# Environment
NODE_ENV=development

# Clerk Authentication
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=your_clerk_publishable_key
CLERK_SECRET_KEY=your_clerk_secret_key

# Next.js
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Stripe (if needed)
STRIPE_SECRET_KEY=your_stripe_secret_key
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key

NEXT_PUBLIC_PLAUSIBLE_DOMAIN=http://localhost:3000

# AI use cli command smart-committer --commit
OPENAI_API_KEY=your_openai_api_key
CLAUDE_API_KEY=your_claude_api_key

RESEND_API_KEY=your_resend_api_key

# CDN URL where your static assets will be served from (e.g., https://cdn.yourapp.com)
NEXT_PUBLIC_CDN_URL=

# Vercel deployment specific (automatically set in Vercel)
VERCEL_GIT_COMMIT_SHA=

# Domain for your application (without protocol, e.g., yourapp.com)
NEXT_PUBLIC_APP_DOMAIN=localhost